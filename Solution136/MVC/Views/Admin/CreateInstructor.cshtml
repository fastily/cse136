@{
    ViewBag.Title = "CreateInstructors";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h3 class="page-header" >Create a new instructor</h3>
<a href="/Admin/InstructorList"><< back</a>
<div id="divInstructor">
    <label>First Name</label>
    <input class="form-control" type="text" data-bind="value: first" /><br />
    <br />
    <label>Last Name</label>
    <input class="form-control" type="text" data-bind="value: last" /><br />
    <br />
    <label>Title</label>
    <input class="form-control" type="text" data-bind="value: title" /><br />
    <br />
    <input class="btn btn-primary" value="Add" type="button" data-bind="click: add" />
    <br /><br />
    <div id="divMessage"></div>
</div>

<script type="text/javascript">
    $(document).ready(function () {
        var instructorViewModelObj = new InstructorViewModel();
        instructorViewModelObj.Initialize();
    });
</script>
