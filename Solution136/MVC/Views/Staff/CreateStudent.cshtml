@{
    ViewBag.Title = "CreateMyStudents";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h3 class="page-header">Create a new student</h3>
<a href="/Staff/StudentList?id=@Session["staffId"]"><< back</a>
<div id="divStudent">
    <label>Id</label>
    <input class="form-control" type="text" data-bind="value: id" /><br />

    <label>First Name</label>
    <input class="form-control" type="text" data-bind="value: first" /><br />

    <label>Last Name</label>
    <input class="form-control" type="text" data-bind="value: last" /><br />

    <label>Email</label>
    <input class="form-control" type="text" data-bind="value: email" /><br />

    <label>Password</label>
    <input class="form-control" type="text" data-bind="value: password" /><br />

    <input class="btn btn-primary" value="Add" type="button" data-bind="click: add" />
    <br />
    <div id="divMessage"></div>
</div>

<script type="text/javascript">
    $(document).ready(function () {
        var studentViewModelObj = new StudentViewModel();
        studentViewModelObj.Initialize();
    });
</script>
