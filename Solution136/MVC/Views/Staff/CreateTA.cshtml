@{
    ViewBag.Title = "CreateMyTas";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h3 class="page-header">Create A New Ta</h3>
<a href="/Staff?id=@Session["staffId"]"><< back</a>
<div id="divTa">
    <label>First Name</label>
    <input class="form-control" type="text" data-bind="value: first" /><br />

    <label>Last Name</label>
    <input class="form-control" type="text" data-bind="value: last" /><br />

    <label>Type</label>
    <input class="form-control" type="text" data-bind="value: type" /><br />

    <input class="btn btn-primary" value="Add" type="button" data-bind="click: add" />
    <br />
    <div id="divMessage"></div>
</div>

<script type="text/javascript">
    $(document).ready(function () {
        var taViewModelObj = new TaViewModel();
        taViewModelObj.Initialize();
    });
</script>
