@{
    ViewBag.Title = "EditMyStudents";
}

<h2 class="page-header">Manage Student</h2>
<a href="/Staff?id=@Session["staffId"]"><< back</a>
<div id="UpdateEnrollment">
    <label>StudentId</label>
    <input class="form-control" type="text" data-bind="value: viewModel.enrollmentUpdate.StudentId" /><br />

    <label>Grade</label>
    <input class="form-control" type="text" data-bind="value: viewModel.enrollmentUpdate.Grade" /><br />

    <input class="btn btn-primary" type="button" data-bind="value: 'UpdateEnrollment', click: viewModel.UpdateEnrollment" />
    <br />
    <div id="divMessage" class="text-success">&nbsp;</div> 
</div>

<script type="text/javascript">
    $(document).ready(function () {
        var enrollmentViewModelObj = new EnrollmentViewModel();
        enrollmentViewModelObj.LoadUpdateGrade("@ViewBag.studentId",@ViewBag.scheduleId);
    });
</script>



