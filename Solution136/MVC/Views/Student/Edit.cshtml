@{

    ViewBag.Title = "Edit";
    var id = ViewBag.id;
}

<h2 class="page-header">Student Detail</h2>
<a href="/Student?id=@Session["adminId"]"><< back</a>
<div id="divStudent">

    <label>Id</label>
    <input class="form-control" type="text" data-bind="value: id" />
    <br />

    <label>First Name</label>
    <input class="form-control" type="text" data-bind="value: first" />
    <br />
    <label>Last Name</label>
    <input class="form-control" type="text" data-bind="value: last" />
    <br />


    <label>Email</label>
    <input class="form-control" type="text" data-bind="value: email" />
    <br />

    <label>Password</label>
    <input class="form-control" type="text" data-bind="value: password" />
    <br />

    <input class="btn btn-primary" value="Update" type="button" />
    <br /><br />
    <div id="divMessage" class="text-success">&nbsp;</div>
</div>


<script type="text/javascript">
    $(document).ready(function () {
        var studentviewModel = new StudentViewModel();
        studentviewModel.Initialize();
    });
</script>
